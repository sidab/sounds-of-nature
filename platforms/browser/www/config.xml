<?xml version='1.0' encoding='utf-8'?>
<widget id="com.soundsofnature.umakhan" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Relax</name>
    <description>
        Sounds Of Nature.
    </description>
    <author email="umakhanr7@yandex.ru" href="https://github.com/sidab/">
        Umakhan Magomedov
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon src="www/icon.png" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="fullscreen" value="false" />
    <preference name="orientation" value="portrait" />
    <preference name="BackgroundColor" value="0xeeeeeeee" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="SplashScreenBackgroundColor" value="0xeeeeeeee" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#007aff" />
    <preference name="AllowInlineMediaPlayback" value="true" />
    <preference name="phonegap-version" value="cli-9.0.0" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="xwalk64bit" value="true" />
    <preference name="CordovaWebViewEngine" value="CDVWKWebViewEngine" />
    <preference name="InterceptRemoteRequests" value="all" />
    <preference name="buildArchitecture" value="arm64" />
</widget>
